<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <%- include('partials/header') %>
    <main class="container py-5" style="max-width: 860px;">
        <h1 class="h3 mb-3">Access eBook</h1>
        <% if (typeof error !== 'undefined' && error) { %>
        <div class="alert alert-danger"><%= error %></div>
        <% } %>
        <form method="post" action="/ebook-access" class="row g-3">
            <div class="col-md-6">
                <label class="form-label">Full Name *</label>
                <input name="fullName" class="form-control" required>
            </div>
            <div class="col-md-6">
                <label class="form-label">Email *</label>
                <input type="email" name="email" class="form-control" required>
            </div>
            <div class="col-md-6">
                <label class="form-label">Mobile Number *</label>
                <input name="mobile" class="form-control" required>
            </div>
            <div class="col-md-6">
                <label class="form-label">College Name</label>
                <input name="college" class="form-control">
            </div>
            <div class="col-md-6">
                <label class="form-label">City</label>
                <input name="city" class="form-control">
            </div>
            <div class="col-md-6">
                <label class="form-label">State</label>
                <input name="state" class="form-control">
            </div>
            <div class="col-12">
                <button class="btn btn-success">Submit &amp; View eBook</button>
            </div>
        </form>
    </main>
    <%- include('partials/footer') %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>

